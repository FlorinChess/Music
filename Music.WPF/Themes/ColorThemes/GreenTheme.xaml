<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="PrimaryColor">#05ac05</Color>
    <SolidColorBrush x:Key="Primary" Color="{StaticResource PrimaryColor}" />
    <SolidColorBrush x:Key="PrimaryLight">#5adf46</SolidColorBrush>
    <SolidColorBrush x:Key="PrimaryDark">#007b00</SolidColorBrush>
</ResourceDictionary>