<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="PrimaryColor">#7e3ff2</Color>
    <SolidColorBrush x:Key="Primary" Color="{StaticResource PrimaryColor}" />
    <SolidColorBrush x:Key="PrimaryLight">#9c47ff</SolidColorBrush>
    <SolidColorBrush x:Key="PrimaryDark">#0000ba</SolidColorBrush>
</ResourceDictionary>